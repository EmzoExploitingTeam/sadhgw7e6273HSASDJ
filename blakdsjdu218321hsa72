Section:NewButton("Bypass Zone ", "ButtonInfo", function()
    local objectNames = {"BlueGK", "Kickoff", "GreenGK", "FreeKick"}

local function destroyObjects()
    for _, objectName in pairs(objectNames) do
        local object = game.Workspace:FindFirstChild(objectName)
        while object do
            object:Destroy()
            object = game.Workspace:FindFirstChild(objectName)
        end
    end
end

while wait(0.01) do
    destroyObjects()
end
end)


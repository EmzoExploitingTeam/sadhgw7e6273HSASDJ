 task.wait()
 for _, player in ipairs(game.Players:GetPlayers()) do
  if player:IsInGroup(groupID) then
   for _, roleName in ipairs(roleNames) do
    if player:GetRoleInGroup(groupID) == roleName then
     User:Kick(player.Name .. " is the noob with role " .. roleName)
    else
     local carpetFloor = game:GetService("Workspace").CarpetFloor; local parts = carpetFloor:GetDescendants() for _, part in ipairs(parts) do if part:IsA("BasePart") then part.Color = Color3.new(math.random(), math.random(), math.random()) end end
    end
   end
  end
 end
end
end)


Section2:NewButton("Safe Position", "ButtonInfo", function()
  -- Define the size and position of the part
local partSize = Vector3.new(100, 1, 100)
local partPosition = Vector3.new(0, 1000000, 0)

-- Check if a part already exists at the given position
local safe  = workspace:FindFirstChild("Safe")
if safe then
    -- If a part exists, teleport the player to it
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(safe.Position) + Vector3.new(0, safe.Size.Y/2 + 5, 0)
else
    -- If no part exists, create a new one
-- Create the part
local part = Instance.new("Part")
part.Size = partSize
part.Name = "Safe"
part.Color = Color3.new(0, 0, 1)
part.Position = partPosition
part.Anchored = true
part.CanCollide = true
part.Parent = game.Workspace

-- Teleport the player to the new location
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(part.Position) + Vector3.new(0, part.Size.Y/2 + 5, 0)
end
end)


